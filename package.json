{
	"name": "live-auto-type",
	"displayName": "live-auto-type",
	"description": "Auto type code from hotkeys",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.38.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:extension.liveAutoType.registerCode1FromClipBoard"
	],
	"main": "./extension.js",
	"contributes": {
		"commands": [
			{
				"command": "extension.liveAutoType.registerCode1FromClipBoard",
				"title": "Live Auto Type: Register Command 1 From Clipboard"
			},
			{
				"command": "extension.liveAutoType.printCode1",
				"title": "Live Auto Type: Print Command 1"
			}
		]
	},
	"scripts": {
		"test": "node ./test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^5.2.6",
		"@types/node": "^10.12.21",
		"@types/vscode": "^1.38.0",
		"eslint": "^5.13.0",
		"glob": "^7.1.4",
		"mocha": "^6.1.4",
		"typescript": "^3.3.1",
		"vscode-test": "^1.2.0"
	}
}
